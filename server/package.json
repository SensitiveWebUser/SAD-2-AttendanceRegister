{
  "name": "attendanceregister-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev -r tsconfig-paths/register src/index.ts --env development",
    "build": "tsc",
    "test": "jest -i --watchAll --no-cache",
    "lint": "",
    "test:ci": "jest -i --no-cache"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "@Routes/(.*)": [
        "<rootDir>/src/routes/$1"
      ],
      "@Middlewares": [
        "<rootDir>/src/middlewares/index.ts"
      ],
      "@Errors": [
        "<rootDir>/src/errors/index.ts"
      ],
      "@Errors/(.*)": [
        "src/errors/$1"
      ],
      "@Models/(.*)": [
        "<rootDir>/src/models/$1"
      ],
      "@Models": [
        "<rootDir>/src/models/index.ts"
      ],
      "@Utils/(.*)": [
        "<rootDir>/src/utils/$1"
      ],
      "@Database": [
        "<rootDir>/src/database/index.ts"
      ],
      "@Controllers": [
        "<rootDir>/src/controllers/index.ts"
      ]
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/setup.ts"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.8",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^7.7.7",
    "express-validator": "^6.14.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^3.0.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.2.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "jest": "^28.1.0",
    "prettier": "2.7.1",
    "sqlite3": "^5.1.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.3",
    "tsconfig-paths": "^4.1.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
